# Threshold for region support in Nodule-Rib image.
regionSupportThreshold	10


# Set to 1 to group candidates otherwise set to 0.
group  1

# The area overlap required for candidate to belong to the same group.
maxOverlap  0.50

# The maximum number of annotations in an image.
maxAnnotations 10



