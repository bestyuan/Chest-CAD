
# The algorithm expects that high density regions in the
# object correspond to low code values in the input image.
# For example, if the input image is displayed as is bones
# appear dark. In this case set invert to 1.
# If the image has been rendered in the standard way so
# that high density regions of the object appear bright
# set invert equal to 0.
invert			0

# The pixel spacing in mm at which the image is processed.
# The input image is resized in order to achieve this aim.
aimPixelSpacing	0.171

# The dimension of ROIs that are cropped from the image.
roiResolution		512


# The maximum number of candidates to be processed from a CAN file.
maxCandidates		140


# The number of templates followed by a list of template files.	 
# The template file paths must be absolute or relative to
# the current working directory.
nNoduleTemplates		2

../data/templates/MediumNoduleTemplate_9_9_2003.tif	2
../data/templates/MediumNoduleTemplate_9_9_2003.tif	3 


# The number of rib templates followed by a list of rib template files.
# The template file paths must be absolute or relative to
# the current working directory.
nRibAggregations	3
nRibTemplates		7
		
